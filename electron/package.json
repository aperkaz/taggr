  },
  "devDependencies": {
    "electron": "^5.0.4",
    "@types/node": "^15.6.1",
    "cpx": "^1.5.0",
    "electron-builder": "^22.10.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  }
