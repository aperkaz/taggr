{
  "name": "taggr",
  "productName": "taggr",
  "version": "0.0.1",
  "description": "Rediscover your memories while keeping your privacy",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "unit": "mocha",
    "unit:watch": "mocha --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": {
    "name": "aperkaz",
    "email": "alainperkaz@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "taggr"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "aperkaz",
              "name": "taggr-releases"
            },
            "authToken": "e8c0863978337e54145b6ab38ab5b99388c6fd03"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/rendererElectron.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.50",
    "@electron-forge/publisher-github": "^6.0.0-beta.51",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "electron": "8.2.1",
    "mocha": "^7.1.1",
    "node-loader": "^0.6.0",
    "react-hot-loader": "^4.12.20",
    "style-loader": "^0.23.1",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@risingstack/react-easy-state": "^6.2.1",
    "@sentry/browser": "^5.15.5",
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow/tfjs": "^1.7.2",
    "@tensorflow/tfjs-converter": "^1.7.2",
    "async": "^3.2.0",
    "comlink": "^4.3.0",
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "jpg-stream": "^1.1.2",
    "lodash.debounce": "^4.0.8",
    "multer": "^1.4.2",
    "node-localstorage": "^2.1.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-window": "^1.8.5",
    "readdirp": "^3.4.0",
    "semver": "^7.3.2",
    "styled-components": "^5.1.0",
    "universal-analytics": "^0.4.20",
    "uuid": "^7.0.3"
  }
}
