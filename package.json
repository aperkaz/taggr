{
  "name": "taggr",
  "productName": "taggr",
  "version": "0.0.1",
  "description": "Rediscover your memories while keeping your privacy",
  "main": "src/entryMainProcess.js",
  "scripts": {
    "start": "env DEVELOPMENT_ENV=false electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "unit": "mocha",
    "unit:watch": "mocha --watch --recursive './src/*/__tests__/*.spec.js'",
    "storybook": "start-storybook -p 6006 ",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": {
    "name": "aperkaz",
    "email": "alainperkaz@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "taggr"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@risingstack/react-easy-state": "^6.2.3",
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow/tfjs": "^1.7.2",
    "@tensorflow/tfjs-node": "^1.7.2",
    "comlink": "^4.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "htm": "^3.0.4",
    "lodash.debounce": "^4.0.8",
    "node-fetch": "^2.6.0",
    "normalize-path": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-window": "^1.8.5",
    "readdirp": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "@storybook/html": "^5.3.18",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "electron": "8.2.0",
    "mocha": "^7.1.1"
  }
}
